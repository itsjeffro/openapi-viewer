    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-router-hash-link": "^2.4.3",
    "react-syntax-highlighter": "^15.5.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.7.12",
